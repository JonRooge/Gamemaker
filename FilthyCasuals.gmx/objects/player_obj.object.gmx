            <kind>1</kind>
            <string>//Movement
if(hp &gt;= 1) {
    lh = gamepad_axis_value(gamepad, gp_axislh);
    lv = gamepad_axis_value(gamepad, gp_axislv);
    lc = abs(lh) + abs(lv);
    
    if(hspeed &lt;= 0.05 &amp;&amp; hspeed &gt;= -0.05) hspeed = 0;
    else if(hspeed &gt; 0.05) hspeed -= 0.05;
    else hspeed += 0.05;
    
    if(vspeed &lt;= 0.05 &amp;&amp; vspeed &gt;= -0.05) vspeed = 0;
    else if(vspeed &gt; 0.05) vspeed -= 0.05;
    else vspeed += 0.05;
    
    image_angle %= 360;
    if(image_angle &lt; 0) image_angle += 360;
    td = image_angle;
    
    if(lc &gt; 0.2) {
        hspeed += lh;
        vspeed += lv;
        td = point_direction(x, y, x + lh, y + lv);
        
        if(hp == 2) {
            p_spr = hp2_spr;
        } else if(hp == 1) {
            p_spr = hp1_spr;
        } else {
            p_spr = player_spr;
        }
    }
    
    if(hspeed == 0 &amp;&amp; vspeed == 0) {
        p_spr = playerStill_spr;
    }

    dif = angleDif_scr(td, image_angle);

    if(dif &gt;= 10) image_angle -= 10;
    else if(dif &lt;= -10) image_angle += 10;
    else if(dif != 0) image_angle = td;
}

//Shoot
if(basicCool &gt; 0) basicCool++;
if(basicCool == 4) basicCool = 0;

if(gamepad_button_check(gamepad, gp_face1) &amp;&amp; basicCool == 0) {
    bullet = instance_create(x, y, basicBullet_obj);
    variance = random_range(-5, 5);
    bullet.image_angle = image_angle + variance;
    bullet.direction = image_angle + variance;
    bullet.speed += abs(hspeed) + abs(vspeed);
    basicCool++;
}

//Hit
if(hp &gt; 0) {
    for (i = 0; i &lt; instance_number(invaderShot_obj); i += 1) {
        bullet = instance_find(invaderShot_obj,i);
        if(place_meeting(x, y, bullet)) {
            instance_create(bullet.x, bullet.y, hit_obj);
            instance_destroy(bullet);
            
            hp--;
            p_ind = 0;
            
            if(hp == 2) {
                p_spr = hp2_spr;
            } else if(hp == 1) {
                p_spr = hp1_spr;
            } else if(hp == 0) {
                p_spr = dead_spr;
            }
        }
    }
}

//show_debug_message(dif);
</string>
